source ~/.dotfiles/zsh/omz.zsh
source ~/.dotfiles/zsh/alias.zsh
if [ -e ./zsh/server.zsh ]; then
  source ~/.dotfiles/zsh/server.zsh
else

# ---- kitty 设置窗口标题 ----
precmd() {
    if [[ -n "$SSH_CONNECTION" ]]; then
        echo -ne "\033]0;${HOST}:${PWD##*/}\007"
    else
        echo -ne "\033]0;${PWD##*/}\007"
    fi
}

